<Window x:Class="dotNetWPF_03_0535_6349.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:dotNetWPF_03_0535_6349"
        mc:Ignorable="d"
        Title="JCT .Net Coures - Exercise No.3 - Refael Guri and Netanel Ovaia" Height="400" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="80*"/>
            <RowDefinition Height="20*"/>
        </Grid.RowDefinitions>
        <Grid Name="prinderGrid">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <local:PrinterUserControl Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Top"/>
            <local:PrinterUserControl Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Top"/>
            <local:PrinterUserControl Grid.Column="2" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        </Grid>
        <Button Grid.Row="1" Content="Print" Margin="10" FontSize="36" Name="printButton"/>

    </Grid>
</Window>
